{
    "AppSettings": {
        "SigningKey": "shared-secret-for-jwt-signing-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Trace",
            "Microsoft.AspNetCore": "Information"
        }
    },
    "AllowedHosts": "*",
    "Storage": {
        "ActiveStorageService": "FileSystemStorageService",
        "FileSystemStorageService": {
            "BasePath": "/var/data",
            "TempFilePath": "/var/tmp"
        }
    }
}