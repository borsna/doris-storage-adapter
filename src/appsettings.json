{
    "AppSettings": {
        "SigningKey": "shared-secret-for-jwt-signing"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Trace",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Storage": {
        "ActiveStorageService": "DiskStorageService",
        "DiskStorageService": {
            "BasePath": "/var/data"
        }
    }
}