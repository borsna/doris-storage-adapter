{
  "AppSettings": {
    "SigningKey": "shared-secret-for-jwt-signing"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LoginRedirectUrl": "https://example.com/login",
  "Storage": {
    "ActiveStorageService": "DiskStorageService",
    "DiskStorageService":{
      "BasePath": "/var/data"
    }
  }
}